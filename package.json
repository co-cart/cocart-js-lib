{
  "name": "@cocart/cocart-rest-api",
  "version": "1.0.0",
  "description": "CoCart - JavaScript Library",
  "author": "Sebastien Dumont",
  "license": "MIT",
  "keywords": [
    "wordpress",
    "woocommerce",
    "rest",
    "cocart",
    "promise",
    "node"
  ],
  "homepage": "https://github.com/co-cart/cocart-js-lib",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/co-cart/cocart-js-lib.git"
  },
  "bugs": {
    "url": "https://github.com/co-cart/cocart-js-lib/issues"
  },
  "main": "index",
  "types": "index.d.ts",
  "exports": {
    "import": "./index.mjs",
    "require": "./index.js"
  },
  "files": [
    "index.js",
    "index.mjs",
    "index.d.ts"
  ],
  "dependencies": {
    "axios": "^0.21.0",
    "create-hmac": "^1.1.7",
    "oauth-1.0a": "^2.2.6",
    "url-parse": "^1.4.7"
  },
  "devDependencies": {
    "@babel/cli": "7.12.8",
    "@babel/core": "7.12.9",
    "@babel/plugin-transform-modules-commonjs": "7.12.1",
    "@babel/preset-env": "7.12.7",
    "babel-jest": "26.6.3",
    "del-cli": "3.0.1",
    "eslint": "7.15.0",
    "eslint-config-prettier": "7.0.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.2.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "5.0.0",
    "husky": "4.3.5",
    "jest": "26.6.3",
    "lint-staged": "10.5.3",
    "nock": "13.0.5",
    "prettier": "2.2.1"
  },
  "scripts": {
    "build": "del index.js && babel index.mjs --out-dir .",
    "test": "jest",
    "format": "prettier --write \"*.mjs\" \"test.js\"",
    "lint": "eslint *.mjs",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A",
    "postversion": "git push && git push --tags"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,mjs}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
